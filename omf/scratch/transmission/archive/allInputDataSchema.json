{
	"modelType": {
		"description": "The type of model loaded.",
		"type": "string",
		"read-only": 1,
		"default": "transmission"
	},
	"modelName": {
		"description": "The name given to the model.",
		"type": "string"
	},
	"user": {
		"description": "The user's name.",
		"type": "string",
		"read-only": 1
	},
	"created": {
		"description": "The model's creation date.",
		"type": "string",
		"read-only": 1,
		"example": "2014-07-30 11:35:33.705000",
	},
	"runTime": {
		"description": "The complete runtime.",
		"type": "string",
		"read-only": 1,
		"example": "0:06:42",
	},
	"networkName": {
		"description": "The name of the network.",
		"type": "string",
		"example": "IEEE 1-Generator Test"
	},
	"algorithm": {
		"description": "The core powerflow algorithm.",
		"type": "string, dropdown",
		"values": "NR,FDXB,FDBX,GS",
		"default": "NR"
	},
	"model": {
		"description": "AC or DC formulation.",
		"type": "string, dropdown",
		"values":"AC,DC",
		"default": "AC"
	},
	"tolerance": {
		"description": "Powerflow termination tolerance.",
		"type": "float, units: P and Q dispatch per unit.",
		"default": "10-8"
	},
	"iteration": {
		"description": "Max. # of iterations for NR/FD/GS.",
		"type": "int",
		"default": "10"
	},
	"genlimits": {
		"description": "Enforce reactive power limits on generators.",
		"type": "int",
		"values": "0,1,2{do not enforce,enforce with simultaneous bus type conversion,enforce with one-at-a-time bus type conversion}",
		"default": "0"
	},	
	"simStartDate": {
		"description": "The date to simulate.",
		"type": "string",
		"default": "2014-01-01"
	},
	"simLength": {
		"description": "The duration to simulate.",
		"type": "integer",
		"default": "100"
	},
	"simLengthUnits": {
		"description": "The units for the duration.",
		"type": "string",
		"default": "hours"
	}
}